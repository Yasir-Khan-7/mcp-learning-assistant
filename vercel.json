{
  "version": 2,
  "framework": null,
  "installCommand": "npm install --prefix demo --ignore-scripts",
  "buildCommand": "echo 'No build needed'",
  "builds": [
    {
      "src": "demo/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "demo/server.js"
    }
  ]
}

